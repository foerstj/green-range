    {
      max_move_velocity = 5;
    }
  [actor]
    {
      [skills]
				{
					strength =  5, 0;
					intelligence =  7, 0;
					dexterity =  7, 0;
					melee =  1,  0;
				}
    }
  [aspect]
    {
      experience_value = 10;
