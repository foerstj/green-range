[t:template,n:butterfly_mp_base]  // = butterfly_mp
{
	category_name = "1w_ambients";
	doc = "MP butterfly";
	specializes = base_ambient;
	[aspect]
	{
		model = m_c_na_bf_pos_1;
	}
	[body]
	{
		initial_chore = chore_default;
		[chore_dictionary]
		{
			chore_prefix = a_c_na_bf_fs0;
			[chore_default]
			{
				skrit = random_infinite_loop;
				[anim_files]
				{
					00 = fl;
					01 = fl-02;
				}
			}
		}
	}
	[common] { screen_name="butterfly"; }
}

