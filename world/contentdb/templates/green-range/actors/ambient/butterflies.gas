[t:template,n:butterfly_base]  // = butterfly
{
	category_name = "1W_ambients";
	doc = "butterfly";
	specializes = actor_ambient;
	[actor] { race=other; }
	[aspect]
	{
		draw_shadow = false;
		model = m_c_na_bf_pos_1;
		is_selectable = false;
	}
	[body]
	{
		avg_move_velocity = 1.000000;
		terrain_movement_permissions = lf_hover;
		[bone_translator]
		{
			body_anterior = Bone01;
			body_mid = Bone01;
			body_posterior = Bone01;
			kill_bone = Bone01;
			shield_bone = Bone01;
			weapon_bone = Bone01;
		}
		[chore_dictionary]
		{
			chore_prefix = a_c_na_bf_fs;
			[chore_attack]
			{
				chore_stances = 0;
				skrit = select_attack;
				[anim_files]
				{
					00 = fl;
					01 = fl-02;
				}
			}
			[chore_default]
			{
				chore_stances = 0;
				skrit = infinite_loop;
				[anim_files] { 00=fl; }
			}
			[chore_die]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files] { 00=fl; }
			}
			[chore_fidget]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files]
				{
					00 = fl;
					01 = fl-02;
				}
			}
			[chore_walk]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files]
				{
					00 = fl;
					01 = fl-02;
				}
			}
		}
	}
	[common] { screen_name="Butterfly"; }
	[mind]
	{
		on_enemy_spotted_attack = false;
		on_enemy_entered_weapon_engage_range_attack = false;
	}
}

