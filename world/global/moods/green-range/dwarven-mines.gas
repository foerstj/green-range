// Moods for Dwarven Mines episode
// mines west
[mood_setting*]
{
// Dwarven Mines
	mood_name				= gr-dm-west-1;
	transition_time			= 1.0f;
	interior				= true;

	[fog]
	{
		fog_near_dist		= 35.0f;
		fog_far_dist		= 45.0f;
		fog_lowdetail_near_dist		= 35.0f;
		fog_lowdetail_far_dist		= 40.0f;
		fog_color			= 0xFF000000;
		fog_density			= 0.5f;
	}

	[music]
	{
		ambient_track		= "";
	  f standard_intro_delay = 8.000000;
	  f standard_repeat_delay = 24.000000;
		standard_track		= s_m_highland_song;
        battle_track = s_m_DwarvenMines_04;
		room_type			= rt_cave;
	}
}
