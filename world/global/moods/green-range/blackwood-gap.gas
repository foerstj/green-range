// Moods for Blackwood Gap episode
[mood_setting*]  // climb south tx
{
	mood_name = gr-bwg-climb-s;
  f transition_time = 8.000000;
  b interior = false;

	[fog]
	{
	  x fog_color = 0xff506070;
	  f fog_density = 0.5;
	  f fog_near_dist = 30.000000;
	  f fog_far_dist = 50.000000;
	  f fog_lowdetail_near_dist = 25.000000;
	  f fog_lowdetail_far_dist = 40.000000;
	}
	[music]
	{
		ambient_track = s_e_ambient_woods_01;
	  f standard_intro_delay = 8.000000;
	  f standard_repeat_delay = 24.000000;
		standard_track = s_m_foreverlands;
        battle_track = s_m_darkforest_03;
		room_type = rt_mountains;
	}
	[rain]
	{
		rain_density = 200.0f;
		lightning = true;
	}
	[wind]
	{
	  f wind_direction = 0.7854;  // NW
	  f wind_velocity = 2.000000;
	}
}
