[effect_script*]
{
	name	=gr_water_elem_die_sm;

    // This is "dsx_elemental_water_hide" just different color
	script	=[[
            set $color .1,.6,.8;  // was 0,.3,1
            set $scale 3;
            set $vmin .5;
            set $vmax 2;
            set $count 5;

			sfx create explosion #TARGET
				"dark()color0($color)color1(0,0,0)offset(0,2,0)
				vmin($vmin)vmax($vmax)omni_dir()scale($scale)radius(0)count($count)";
			sfx snap_to_ground #PEEK;
			sfx start #PEEK;

			sfx create explosion #TARGET
				"dark()color0($color)color1(0,0,0)offset(.2,1.8,.2)
				vmin($vmin)vmax($vmax)omni_dir()scale($scale)radius(0)count($count)";
			sfx snap_to_ground #PEEK;
			sfx start #PEEK;
			sfx create explosion #TARGET
				"dark()color0($color)color1(0,0,0)offset(.2,1.8,-.2)
				vmin($vmin)vmax($vmax)omni_dir()scale($scale)radius(0)count($count)";
			sfx snap_to_ground #PEEK;
			sfx start #PEEK;
			sfx create explosion #TARGET
