/*
# seed factor (seeds/mÂ²), perlin offset, perlin spread, templates, size (from, to, additional perlin factor)
*/

[perlin_plant_profile]
{
    // defaults
    size = 0.8 1.2;

    // trunks: none
    // trees
    [*] { seed = 0.1, 0, 3; templates = tree_des_02 tree_des_pine_01 tree_des_pine_02; }
    // bushes
    [*] { seed = 0.2, 0, 3; templates = bush_des_02 bush_des_03 bush_des_04 bush_des_06 tree_des_dead_03; }
    // groundcover
    [*] { seed = 0.3, 0, 3; templates = brush_des_dry_01 brush_des_dry_02 brush_des_dry_03; }
}
